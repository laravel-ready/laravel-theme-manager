{
    "name": "laravel-ready/theme-manager",
    "description": "Theme manager for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "theme manager"
    ],
    "authors": [
        {
            "name": "EgoistDeveloper",
            "email": "epsilonelittr@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel-ready/theme-manager/issues",
        "source": "https://github.com/laravel-ready/theme-manager"
    },
    "require": {
        "php": "^8.1 || ^8.0",
        "illuminate/support": "^10.0 || ^9.0 || ^8.0",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^v8.0.8",
        "phpstan/phpstan-phpunit": "^1.3.10"
    },
    "autoload": {
        "psr-4": {
            "LaravelReady\\ThemeManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelReady\\ThemeManager\\ThemeServiceProvider"
            ],
            "aliases": {
                "ThemeManager": "LaravelReady\\ThemeManager\\Facades\\ThemeManager"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
